{
  "name": "vue3-google-map",
  "private": true,
  "version": "0.0.0",
  "description": "Google Map with Vue3",
  "author": "CofCat <a25690756@gmail.com> (https://github.com/CofCat456)",
  "type": "module",
  "scripts": {
    "build:core": "pnpm --filter @vue3-google-map/core build",
    "build:types": "pnpm --filter @vue3-google-map/types build",
    "build:map": "pnpm --filter @cofcat/vue3-google-map build",
    "lint": "eslint './packages/**/*.{ts,vue}'",
    "lint:fix": "pnpm lint --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "{packages}/**/*.{ts,vue}": [
      "pnpm lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/fs-extra": "^11.0.1",
    "@types/google.maps": "^3.53.5",
    "@types/node": "^20.4.5",
    "consola": "^3.2.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.1.6",
    "vitepress": "1.0.0-beta.7",
    "vue": "^3.3.4"
  }
